            @Html.DisplayFor(model => model.Employee.Pay)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="Employee.EId" />
        <input type="submit" value="Fire" class="btn btn-danger" /> 
         <a class="button primary" asp-page="./Index">Back to List</a>
