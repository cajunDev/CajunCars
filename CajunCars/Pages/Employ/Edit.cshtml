<body class="is-preload">

<!-- Page Wrapper -->
<div id="page-wrapper">

   
<h1>Transfer Employee</h1>

<hr/>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Employee.EId"/>
            <p>Enter New DealershipID to transfer Employee</p>
            <div class="form-group">
                <label asp-for="Employee.DId" class="control-label"></label>
                <input asp-for="Employee.DId" class="form-control"/>
                <span asp-validation-for="Employee.DId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Employee.Name" class="control-label"></label>
                <input asp-for="Employee.Name" class="form-control"/>
                <span asp-validation-for="Employee.Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Employee.Position" class="control-label"></label>
                <input asp-for="Employee.Position" class="form-control"/>
                <span asp-validation-for="Employee.Position" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Employee.Pay" class="control-label"></label>
                <input asp-for="Employee.Pay" class="form-control"/>
                <span asp-validation-for="Employee.Pay" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary"/>
            </div>
        </form>
    </div>
</div>

    <div>
        
         <a class="button primary" asp-page="./Index">Back to List</a>
        
